{
  "name": "area17/twill-capsule-feature-flags",
  "description": "A Twill Capsule to handle application feature flags",
  "homepage": "https://github.com/area17/twill-capsule-feature-flags",
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "area17/twill": "^2.7"
  },
  "require-dev": {
      "nunomaduro/larastan": "^2.0",
      "phpstan/phpstan": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "A17\\TwillFeatureFlags\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "A17\\TwillFeatureFlags\\ServiceProvider"
      ]
    }
  },
  "scripts": {
      "post-autoload-dump": [
          "npx husky install"
      ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
